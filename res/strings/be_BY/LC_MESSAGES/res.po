msgstr "Сэнт-Клаўд"

msgid "MISSION_TWENTIES_PARAMS_STPAUL"
msgstr "Сэнт-Пол"

msgid "MISSION_TWENTIES_PARAMS_TRAININGSCENTER"
msgstr "Навучальны цэнтар"

